{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development tsc && concurrently \"npm:build:watch\" \"nodemon\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "tsc",
    "build:watch": "tsc --watch --preserveWatchOutput --pretty",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@azure/cosmos": "^3.10.3",
    "dotenv": "^8.2.0",
    "fastify": "^3.6.0",
    "fastify-cors": "^4.1.0",
    "fastify-swagger": "^4.4.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "@types/uuid": "^8.3.0",
    "concurrently": "^5.3.0",
    "jest": "^26.5.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.7",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3",
    "webpack": "^5.3.2",
    "webpack-node-externals": "^2.5.2"
  }
}
